package Demo;

public class Demo1 {
	public static void main(String[] args) {
	String str="madam";
	String rev="";
	
	 for(int j=str.length()-1;j>=0;j--) {
			rev=rev+str.charAt(j);
			    }
		     
		    if(rev.equals(str))
		    	System.out.println("palindrom");
		    else {
		    	System.out.println("not palindrom");
		    }
	
}
}


	

