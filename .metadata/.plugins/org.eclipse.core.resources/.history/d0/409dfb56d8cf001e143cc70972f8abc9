package Demo;

public class Demo2 {
    public static void main(String[] args) {
        int n=121;
        int num=n;
        int rev=0;
        int last;
        
      while(n>0) {
    	 last=n%10;
    	 rev=rev*10+last;
    	 n=n/10;
      }
       System.out.println(rev);
       if(rev == num)
    	   System.out.println("palindrom");
	    else {
	    	System.out.println("not palindrom");
	    }
    }
}
