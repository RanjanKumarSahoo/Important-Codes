package Demo;

public class Demo1 {

	public static void main(String[] args) {
		int n=101;
		int r=0;
		int last;
		int originalN = n;
		
		while(n>0) {
			last=n%10;
			r=r*10+last;
			n=n/10;
		}
		
		System.out.println(r);
		if(originalN ==r) {
			System.out.println("palindrom");}
		else {
			System.out.println("not palindrom");
		}
	}
}
