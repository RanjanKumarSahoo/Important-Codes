package Demo;

import org.openqa.selenium.By;
import org.openqa.selenium.WebDriver;
import org.openqa.selenium.chrome.ChromeDriver;

public class Demo {
public static void main(String[] args) {
	int n=12345;
	int rev=0;
	int lastnum;
	while(n>0) {
		lastnum=n%10;
		rev=rev*10+lastnum;
		n=n/10;
	}
	System.out.println(rev);
}
}
