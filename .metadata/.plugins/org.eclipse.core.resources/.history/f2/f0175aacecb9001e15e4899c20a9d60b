package Programming;
public class ReverseAStringWithoutChangingPlace {
		 public static void main(String[] args) { 
		   
		 // 1- Using String concatenation operator 
		  
		 String s="Hello World"; 
		 String rev=""; 
		 String n="";
		 int length=s.length(); 
		  
		 for(int i=4;i>=0;i--) 
		 { 
		 rev=rev+s.charAt(i);
		 }
		 System.out.println(rev);
	}
}




