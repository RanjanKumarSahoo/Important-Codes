package Demo;

public class Demo1 {
	    public static void main(String[] args) {
	        String input = "Hello World";
	        String[] parts = input.split("\\s+"); // Split the input string by whitespace
	        String part1 = parts[0]; // First part
	        String part2 = parts[1]; // Second part
	        String rev1="";
	        String rev2="";

	        for (int i=part1.length()-1;i>=0;i--) {
	        	rev1=rev1+part1.charAt(i);
	        }
	        for (int j=part2.length()-1;j>=0;j--) {
	        	rev2=rev2+part2.charAt(j); 
	    }
	        System.out.println(rev1+" "+rev2);
	}
}



	

