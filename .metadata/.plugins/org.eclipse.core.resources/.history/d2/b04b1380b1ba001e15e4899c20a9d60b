package Demo;
import java.util.HashMap;
import java.util.Map;
import java.util.TreeMap;

	import org.apache.poi.ss.usermodel.*;

	import java.io.FileInputStream;
	import java.io.IOException;

	public class Demo {

	    public static void main(String[] args) {
	        try {
	            String excelFilePath = "path/to/credential.xls";
	            String username = readExcelData(excelFilePath, "Sheet1", 1, 0); 
	            String password = readExcelData(excelFilePath, "Sheet1", 1, 1); 
	            login(username, password);
	        } catch (IOException e) {
	            e.printStackTrace();
	        }
	    }

	    public static String readExcelData(String excelFilePath, String sheetName, int rowNumber, int columnNumber) throws IOException {
	        FileInputStream fileInputStream = new FileInputStream(excelFilePath);
	        Workbook workbook = WorkbookFactory.create(fileInputStream);
	        Sheet sheet = workbook.getSheet(sheetName);
	        Row row = sheet.getRow(rowNumber);
	        Cell cell = row.getCell(columnNumber);
	        fileInputStream.close();

	        return cell.getStringCellValue();
	    }

	    public static void login(String username, String password) {
	        System.out.println( username + " and password: " + password);
	    }
	}


