package Demo;

import java.util.Arrays;

public class Demo2 {
    public static void main(String[] args) {

    		
	        String input = "98069032";
	        char[] digits = input.toCharArray();

	        // Sort the digits except for the first one
	        Arrays .sort(digits, 1, digits.length);

	        // Print the modified number
	        System.out.println(new String(digits));
	    }
}
