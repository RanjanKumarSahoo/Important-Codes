package Programming;

public class ReverseAStringWithoutChangingPlace {
	    public static void main(String[] args) {

	        // Using String concatenation operator

	        String s = "Hello World";
	        String rev = "";
	        int length = s.length();

	        for (int i = 0; i < length; i++) {
	            rev = s.charAt(i) + rev;
	        }

	        System.out.println(rev);
	    }
	}



